{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app.js"
    }
  ],

  "functions": {
      "api/test.js": {
        "maxDuration": 30 
      },
      "api/*.js": {
        "maxDuration": 15
      },
      "src/api/*.js": {
        "maxDuration": 25 
      }
    }
  
}


